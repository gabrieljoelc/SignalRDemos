<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="MessageForwardingInCaseOfFaultConfig" type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core" />
    <section name="TransportConfig" type="NServiceBus.Config.TransportConfig, NServiceBus.Core" />
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
  </configSections>
  
  <connectionStrings>
    <!--RabbitMQ connectionString-->
    <add name="NServiceBus/Transport" connectionString="host=localhost"/>
  </connectionStrings>
  
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="SignalRDemos.Messages.Server.Error" />
  <TransportConfig />
  <UnicastBusConfig ForwardReceivedMessagesTo="SignalRDemos.Messages.Server.Audit">
    <MessageEndpointMappings>
      <add Messages="SignalRDemos.Messages" Endpoint="SignalRDemos.Messages.Server" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="RabbitMQ.Client" publicKeyToken="89e7d7c5feba84ce" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.4.0" newVersion="3.0.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>